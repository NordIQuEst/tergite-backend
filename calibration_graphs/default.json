{
  "fidelity": {
    "dependencies": [
      "rabi"
    ],
    "calibration_fn": "calibrate_fidelity",
    "check_fn": "check_fidelity",
    "fidelity_measurement": true,
    "params": [
      {
        "name": "fid",
        "unit": "N/A"
      }
    ]
  },
  "rabi":{
    "dependencies": [
      "two-tone",
      "resonator_spectroscopy"
    ],
    "calibration_fn": "calibrate_rabi",
    "check_fn": "check_rabi",
    "params": [
      {
        "name": "t1",
        "unit": "s"
      },
      {
        "name": "res_freq",
        "unit": "Hz"
      }
    ]
  },

  "resonator_spectroscopy":{
    "dependencies": [],
    "calibration_fn": "calibrate_resonator_spectroscopy",
    "check_fn": "check_resonator_spectroscopy",
    "params": [
      {
        "name": "res_freq",
        "unit": "Hz"
      }
    ]
  },

  "two-tone":{
    "dependencies": ["resonator_spectroscopy"],
    "calibration_fn": "calibrate_two-tone",
    "check_fn": "check_two-tone",
    "params": [
      {
        "name": "drive_freq",
        "unit": "Hz"
      }
    ]
  }
}
