{
  "fidelity": {
    "dependencies": [
      "rabi"
    ],
    "calibration_fn": "calibrate_fidelity",
    "check_fn": "check_fidelity",
    "fidelity_measurement": true,
    "params": [
      {
        "name": "fid",
        "unit": "N/A",
        "threshold_upper": 1.0001,
        "threshold_lower": 0.9999,
        "timeout": 30
      }
    ]
  },
  "rabi":{
    "dependencies": [
      "two-tone",
      "resonator_spectroscopy"
    ],
    "calibration_fn": "calibrate_rabi",
    "check_fn": "check_rabi",
    "params": [
      {
        "name": "t1",
        "unit": "s",
        "threshold_upper": 22e-6,
        "threshold_lower": 20e-6,
        "timeout": 1000
      },
      {
        "name": "res_freq",
        "unit": "Hz",
        "threshold_upper": 1.1e6,
        "threshold_lower": 1e6,
        "timeout": 700
      }
    ]
  },

  "resonator_spectroscopy":{
    "dependencies": [],
    "calibration_fn": "calibrate_resonator_spectroscopy",
    "check_fn": "check_resonator_spectroscopy",
    "params": [
      {
        "name": "res_freq",
        "unit": "Hz",
        "threshold_upper": 1.1e6,
        "threshold_lower": 1e6,
        "timeout": 2000
      }
    ]
  },

  "two-tone":{
    "dependencies": ["resonator_spectroscopy"],
    "calibration_fn": "calibrate_two-tone",
    "check_fn": "check_two-tone",
    "params": [
      {
        "name": "drive_freq",
        "unit": "Hz",
        "threshold_upper": 1.1e6,
        "threshold_lower": 1e6,
        "timeout": 10
      }
    ]
  }
}
